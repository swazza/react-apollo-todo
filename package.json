{
  "name": "react-apollo-todo",
  "version": "1.0.0",
  "description": "Todo With React Apollo",
  "main": "index.js",
  "repository": "git@github.com:swazza/react-apollo-todo.git",
  "author": "swazza <rup@swazza.io>",
  "license": "MIT",
  "scripts": {
    "buildClient": "rm -rf ./dist/client && webpack --config ./build/webpack.client.config.js --env.dev",
    "startClient": "rm -rf ./dist/client && webpack-dev-server --config ./build/webpack.client.config.js --env.dev",
    "buildServer": "rm -rf ./dist/server && webpack --config ./build/webpack.server.config.js --env.dev",
    "startServer": "concurrently 'webpack --watch --config ./build/webpack.server.config.js --env.dev' 'nodemon ./dist/server/app.server.bundle.js'"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-jsx": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.29.0",
    "nodemon": "^1.11.0",
    "noop-webpack-plugin": "^1.0.1",
    "react-hot-loader": "next",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "aphrodite": "^1.2.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "graphql": "^0.10.4",
    "graphql-anywhere": "^3.1.0",
    "graphql-server-express": "^1.0.2",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.1.0",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.2",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.7.1",
    "redux-saga": "^0.15.4",
    "subscriptions-transport-ws": "^0.8.1"
  }
